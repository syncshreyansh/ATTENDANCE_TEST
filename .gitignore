# Untrack the venv folder
git rm -r --cached venv/

# Untrack the .vscode folder (editor settings)
git rm -r --cached .vscode/

# Untrack the instance folder (often holds local databases)
git rm -r --cached instance/

# Untrack any __pycache__ directories that got added
git rm -r --cached **/__pycache__/

# Untrack any .dat files (if they are large models)
git rm -r --cached **/*.dat